<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oms_infotech.www.gmatguru">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Cloud"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Section" />
        <activity android:name=".Mode" />
        <activity android:name=".Practice" />
        <activity android:name=".Quiz" />
        <activity android:name=".DifficultyLevel" />
        <activity android:name=".FinishQuiz" />
        <activity android:name=".TestYourself"></activity>
    </application>

</manifest>